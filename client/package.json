{
  "name": "flatblocks-client",
  "description": "Flatblocks front-end client",
  "version": "0.0.1",
  "license": "BSD-3-Clause",
  "main": "dist/server.js",
  "scripts": {
    "test": "karma start configs/karma.js",
    "start": "forever --minUptime 1000 --spinSleepTime 1000 -c \"node --harmony\" ./dist/server.js",
    "build-server": "webpack --colors --display-error-details --config configs/webpack.server.js",
    "build-client": "webpack --colors --display-error-details --config configs/webpack.client.js",
    "build": "npm run build-server && npm run build-client",
    "watch-server": "webpack --watch --verbose --colors --display-error-details --config configs/webpack.server-watch.js",
    "watch-server-start": "just-wait --pattern \"dist/*.js\" && npm run start",
    "watch-client": "webpack-dev-server --config configs/webpack.client-watch.js",
    "watch": "concurrently --kill-others \"npm run watch-server-start\" \"npm run watch-server\" \"npm run watch-client\""
  },
  "dependencies": {
    "axios": "^0.10.0",
    "babel-polyfill": "6.5.0",
    "baobab": "^2.3.3",
    "baobab-prop-types": "^1.0.0",
    "baobab-react": "^1.0.1",
    "baobab-react-mixins": "^1.0.0",
    "baobab-react-schemabranchmixin": "^1.0.9",
    "classnames": "^2.2.3",
    "fetch-plus": "3.6.1",
    "fetch-plus-bearerauth": "3.5.0",
    "fetch-plus-json": "3.6.0",
    "file-loader": "0.8.5",
    "isomorphic-fetch": "2.2.1",
    "jquery": "^2.2.3",
    "koa": "~2.0.0",
    "koa-convert": "~1.2.0",
    "koa-proxy": "~0.4.1",
    "koa-static": "~2.0.0",
    "lodash": "^4.6.1",
    "nconf": "~0.8.4",
    "react": "0.14.7",
    "react-addons-css-transition-group": "0.14.7",
    "react-dom": "0.14.7",
    "react-masonry-component": "^4.0.2",
    "react-router": "2.0.0",
    "react-google-maps": "^4.10.1",
    "redbox-react": "^1.2.3",
    "transform-keys": "0.0.3",
    "webpack": "~1.12.15",
    "url-loader": "^0.5.7",
    "react-slick": "^0.12.0",
    "scroll-into-view": "^1.6.0",
    "webpack-polyfills-plugin": "0.0.9"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel": "6.5.2",
    "bootstrap": "^3.3.6",
    "babel-core": "6.5.2",
    "babel-loader": "6.2.3",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.0",
    "babel-preset-stage-0": "6.5.0",
    "concurrently": "~2.0.0",
    "css-loader": "^0.23.1",
    "forever": "0.15.1",
    "json-loader": "0.5.4",
    "just-wait": "1.0.5",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack-dev-server": "~1.14.1",
    "webpack-node-externals": "0.4.1",
    "chai": "^3.4.0",
    "isparta-loader": "^2.0.0",
    "karma": "^0.13.14",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-phantomjs-shim": "^1.1.2",
    "karma-sinon": "^1.0.4",
    "karma-sinon-chai": "^1.1.0",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.3",
    "phantomjs": "^1.9.17",
    "react-addons-test-utils": "0.14.7",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  },
  "engines": {
    "node": ">=0.10.32"
  }
}
